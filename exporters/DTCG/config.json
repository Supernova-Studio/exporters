[
    {
        "key": "generateEmptyFiles",
        "type": "boolean",
        "default": false,
        "title": "Generate Empty Files",
        "description": "When enabled, empty style files will be generated. Otherwise empty are omitted",
        "category": "Structure"
    },
    {
        "key": "generateSingleFile",
        "type": "boolean",
        "default": false,
        "title": "Generate Single File",
        "description": "When enabled, a single file will be generated for all tokens.",
        "category": "Structure"
    },
    {
        "key": "inlineThemes",
        "type": "boolean",
        "default": true,
        "title": "Inline Themes",
        "description": "When enabled, themes will be inlined into the generated definition files under $themes key. Otherwise, themes will be generated as completely separate definitions, with themes applied.",
        "category": "Structure"
    },
    {
        "key": "themeOverridesOnly",
        "type": "boolean",
        "default": true,
        "title": "Theme Overrides Only",
        "description": "When enabled, theme values will be generated only for tokens that have overrides applied. Only changes output when `inlineThemes` is disabled.",
        "category": "Output"
    },
