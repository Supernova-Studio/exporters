[
    {
        "key": "showGeneratedFileDisclaimer",
        "type": "boolean",
        "default": true,
        "title": "Show Generated File Disclaimer",
        "description": "When enabled, a disclaimer showing the fact that the file was generated automatically and should not be changed manually will appear in all style styles"
    },
    {
        "key": "disclaimer",
        "type": "string",
        "default": "This file was generated by Supernova and should not be changed manually",
        "title": "Disclaimer",
        "description": "When enabled, a disclaimer showing the fact that the file was generated automatically and should not be changed manually will appear in all style styles"
    },
    {
        "key": "generateIndexFile",
        "type": "boolean",
        "default": true,
        "title": "Generate Index File",
        "description": "When enabled, file with all css style files imported will be generated"
    },
    {
        "key": "generateEmptyFiles",
        "type": "boolean",
        "default": false,
        "title": "Generate Empty Files",
        "description": "When enabled, empty style files will be generated. Otherwise empty are omitted"
    },
    {
        "key": "showDescriptions",
        "type": "boolean",
        "default": true,
        "title": "Show Descriptions",
        "description": "When enabled, token description will be shown as code comments for every exported token"
    },
    {
        "key": "useReferences",
        "type": "boolean",
        "default": true,
        "title": "Use References",
        "description": "When enabled, values will use references to other tokens where applicable"
    },
    {
        "key": "tokenNameStyle",
        "type": "enum",
        "options": [
            "camelCase",
            "capitalCase",
            "constantCase",
            "dotCase",
            "headerCase",
            "noCase",
            "paramCase",
            "pascalCase",
            "pathCase",
            "sentenceCase",
            "snakeCase"
        ],
        "default": "camelCase",
        "title": "Token Name Style",
        "description": "Style of exported token names"
    },
    {
        "key": "colorFormat",
        "type": "enum",
        "options": [
            "rgb",
            "rgba",
            "smartRgba",
            "hex6",
            "hex8",
            "hashHex6",
            "hashHex8",
            "smartHashHex",
            "smartHex",
            "hsl",
            "hsla",
            "smartHsla",
            "smartUIColor"
        ],
        "default": "smartHashHex",
        "title": "Color Format",
        "description": "Format of the exported colors"
    },
    {
        "key": "colorPrecision",
        "type": "number",
        "default": 3,
        "title": "Color Precision",
        "description": "Max number of decimals in colors"
    },
    {
        "key": "indent",
        "type": "number",
        "default": 2,
        "title": "Indentation",
        "description": "Number of spaces used to indent every css variables"
    },
    {
        "key": "newLines",
        "type": "number",
        "default": 1,
        "title": "New Lines",
        "description": "Number of new lines used after every token "
    },
    {
        "key": "indexFileName",
        "type": "string",
        "default": "index.css",
        "title": "Index File Name",
        "description": "Name of the index file that will be generated"
    },
    {
        "key": "baseStyleFilePath",
        "type": "string",
        "default": "./styles",
